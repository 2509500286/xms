    <script>
        require(['jquery', 'notice', 'home.index', 'bootstrap', 'jqueryBootstrap'
            , 'mousewheel'
            , 'jquery.ui'
            , 'jquery.ui.widget'
            , 'jquery.ui.mouse'
            , 'jquery.ui.draggable'
            , 'xms.jquery'
            , 'xms.web'
            , 'fetch'
            , 'xmsMain.plugs'
            , 'xms-history'
            , 'jquery.toast'
            , 'jquery.tmpl'
            , 'jquery.cookie'
        ], function ($, notice, indexPage) {

            $(function () {
                if (top.location != location) {
                    top.location.href = location.href;
                }

                //页面初始化
                indexPage.init();

                //消息通知
                notice.init();
                notice.bindEvent();
            });

        });

        //用于与子页面通讯
        function triggerSubpageHandler(fn) {
            fn && fn(window);
        }
        function iframeReady(obj) {
            var timer = setInterval(function () {
                if (typeof SetWinHeight === "undefined") {
                    return false;
                }
                SetWinHeight(this);
                clearInterval(timer);
            }, 50);
        }

    </script>
