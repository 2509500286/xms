    <style>
        /*消息通知*/
        .newslist {
            display: none;
            border: 1px solid #dddddd;
            width: 260px;
            cursor: pointer;
            position: fixed;
            z-index: 999;
        }

        .list-group {
            max-height: 200px;
            overflow: auto;
        }

        .newslist .list-group li:hover {
            background: #336199;
            color: #ffffff;
        }

        .newslist .list-group li span {
            font-weight: 600;
            padding-right: 8px;
        }

        .newslist .list-group li i {
            float: right;
            text-align: right;
            color: #cccccc;
            font-size: 8px;
            line-height: 20px;
        }

        .newslist a {
            border: 1px solid #dddddd;
            border-bottom: 0;
            display: block;
            width: 100%;
            height: 40px;
            line-height: 40px;
            margin-top: -20px;
            text-align: center;
            background: #ffffff;
        }

        #newsDatasarea {
            cursor: pointer;
            z-index: 9999;
            padding: 8px;
            position: absolute;
        }

        #newsData {
            min-width: 17px;
            height: 17px;
            padding: 1px 5px;
            border-radius: 8px;
            background: #c5782a;
            position: static;
            line-height: 17px;
            text-align: center;
        }

        iframe {
            margin: 0px;
            float: left;
        }

        #noticeCenterBtn {
            position: relative;
        }

            #noticeCenterBtn .tooltip {
                position: absolute;
                left: 10px;
                top: -2px;
                opacity: 1;
                font-size: 12px;
            }

        .nav-right-toolbar .popover {
            font-size: 12px;
        }

            .nav-right-toolbar .popover .notice-list {
                white-space: nowrap;
                list-style: none;
                padding: 0;
            }

            .nav-right-toolbar .popover .notice-list-item {
                padding: 4px 0;
                margin: 0;
                line-height: 18px;
            }

                .nav-right-toolbar .popover .notice-list-item > a {
                    color: #555;
                }

            .nav-right-toolbar .popover li.notice-list-item:hover > a {
                background: #fafafa;
                text-decoration: none;
                color: #555;
            }

            .nav-right-toolbar .popover .notice-list-item > a > div {
                position: relative;
                width: 130px;
                display: table-cell;
            }

                .nav-right-toolbar .popover .notice-list-item > a > div:last-child {
                    /*float: right;*/
                    margin-top: -18px;
                    display: table-cell;
                }

                .nav-right-toolbar .popover .notice-list-item > a > div > span:first-child {
                    width: 15px;
                    position: absolute;
                    top: 3px;
                }

                .nav-right-toolbar .popover .notice-list-item > a > div > span:last-child {
                    vertical-align: middle;
                    line-height: 18px;
                    height: 18px;
                    display: block;
                    padding-left: 18px;
                    white-space: nowrap;
                    overflow: hidden;
                    width: 110px;
                    text-overflow: ellipsis;
                }

        #noticeCenterBtn .tooltip-inner {
            padding: 0px 5px;
        }
    </style>
    <link id="themeLink" href="~/content/css/theme/@(app.Theme).css" rel="stylesheet" />
    @{await Html.RenderPartialAsync("CommonLabel"); }

</head>
